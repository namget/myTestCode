<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/shippingFragment">

    <fragment
        android:id="@+id/shippingFragment"
        android:name="com.bodyfriend.waterpurifier.ui.shipping.ShippingFragment"
        android:label="ShippingFragment"
        tools:layout="@layout/fragment_shipping" >
        <action
            android:id="@+id/action_shippingFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.bodyfriend.waterpurifier.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home"/>
</navigation>